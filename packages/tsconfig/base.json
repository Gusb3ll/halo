{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Default",
  "compilerOptions": {
    "strict": true,
    "moduleResolution": "node",
    "skipLibCheck": true,
    "noImplicitAny": false,
    "esModuleInterop": true,
    "strictNullChecks": false,
    "forceConsistentCasingInFileNames": true
  },
  "exclude": ["node_modules"]
}
